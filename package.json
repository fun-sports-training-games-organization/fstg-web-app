{
    "name": "fstg",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "@emotion/css": "^11.1.3",
        "@emotion/react": "^11.4.1",
        "@emotion/styled": "^11.3.0",
        "@fontsource/material-icons": "^4.5.0",
        "@mui/icons-material": "^5.0.3",
        "@mui/material": "^5.0.3",
        "@mui/styles": "^5.0.1",
        "@mui/system": "^5.0.3",
        "@testing-library/jest-dom": "^5.14.1",
        "@testing-library/react": "^11.2.7",
        "@testing-library/user-event": "^12.8.3",
        "@types/node": "^12.20.23",
        "@types/react": "^17.0.19",
        "@types/react-dom": "^17.0.9",
        "firebase": "^9.1.3",
        "i18next": "^21.1.0",
        "i18next-browser-languagedetector": "^6.1.2",
        "i18next-http-backend": "^1.3.1",
        "notistack": "^2.0.2",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-firebaseui": "^5.0.2",
        "react-i18next": "^11.12.0",
        "react-redux": "^7.2.5",
        "react-router-dom": "^5.3.0",
        "react-scripts": "4.0.3",
        "react-swipeable-views": "^0.14.0",
        "redux-devtools-extension": "^2.13.9",
        "redux-thunk": "^2.3.0",
        "typescript": "^4.4.4",
        "web-vitals": "^1.1.2"
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "jest --no-cache",
        "test:coverage": "jest --no-cache --collectCoverage=true",
        "test:watch": "jest --watch",
        "e2e": "cypress run --spec cypress/integration/fstg/**/*spec.ts",
        "e2e:watch": "cypress open --config ignoreTestFiles=**/cypress/integration/examples/**",
        "e2e-examples": "cypress run --spec cypress/integration/examples/**/*spec.ts",
        "e2e-examples:watch": "cypress open --config ignoreTestFiles=!**/cypress/integration/examples/**",
        "e2e-all": "cypress run --spec cypress/integration/**/*spec.ts",
        "e2e-all:watch": "cypress open --config ignoreTestFiles=!**/cypress/integration/**/*spec.ts",
        "lint": "CI=true && tsc --noEmit && eslint -f table 'src/**/*.{ts,tsx}'",
        "lint:html": "CI=true && tsc --noEmit && eslint -f html -o .doc/html-report/lint_report.html 'src/**/*.{ts,tsx}'",
        "lint:fix": "tsc --noEmit && eslint 'src/**/*.{ts,tsx}' --fix",
        "storybook": "start-storybook -p 6006",
        "storybook:export": "build-storybook",
        "eject": "react-scripts eject",
        "update-browserlist-db": "npx browserslist@latest --update-db"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@firebase/rules-unit-testing": "^2.0.1",
        "@react-theming/storybook-addon": "^1.1.1",
        "@storybook/addon-backgrounds": "^6.3.12",
        "@storybook/addon-controls": "^6.3.12",
        "@storybook/addon-docs": "^6.3.12",
        "@storybook/addon-essentials": "^6.3.12",
        "@storybook/addon-links": "^6.3.12",
        "@storybook/addon-viewport": "^6.3.12",
        "@storybook/react": "^6.3.12",
        "@storybook/theming": "^6.3.12",
        "@testing-library/cypress": "^8.0.1",
        "@types/firebase": "^3.2.1",
        "@types/jest": "^27.0.2",
        "@types/react-redux": "^7.1.18",
        "@types/react-router-dom": "^5.1.8",
        "@types/react-swipeable-views": "^0.13.1",
        "@types/testing-library__cypress": "^5.0.9",
        "@typescript-eslint/eslint-plugin": "^4.31.0",
        "@typescript-eslint/parser": "^4.31.0",
        "babel-jest": "^26.6.0",
        "cypress": "^8.7.0",
        "eslint": "^7.32.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-react": "^7.25.1",
        "eslint-plugin-unused-imports": "^1.1.4",
        "jest": "^26.6.0",
        "jest-environment-jsdom": "^27.0.6",
        "jest-stare": "^2.3.0",
        "prettier": "^2.3.2",
        "sass": "^1.42.1",
        "storybook-react-i18next": "^1.0.12",
        "ts-jest": "^27.0.7"
    }
}
